<%- include('partials/header') %>
<body>
  <%- include('partials/navbar') %>

  <div class="overflow-x-auto">
    <table class="table table-zebra">
      <!-- head -->
      <thead>
        <tr>
          <th>ID</th>
          <th>Position</th>
        </tr>
      </thead>
      <tbody>
        <% positions.forEach((position) => { %>
          <tr>
            <td><a class="btn" href="/positions/<%=position.id%>"><%=position.id%></a></td>
            <td><%=position.name%></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </div>


</body>

</html>